{
  "presets": [
    ["env", { "modules": false }],
    "react",
  ],

  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-es2015-modules-commonjs",
    ["transform-imports", {
      "lodash": {
        "transform": "lodash/${member}",
        "preventFullImport": true
      }
    }]
  ]
}
